            <choice>
               <ref name="correspondence_model.labelLike"/>
               <ref name="correspondence_model.placeNamePart"/>
               <ref name="correspondence_model.offsetLike"/>
               <ref name="correspondence_model.measureLike"/>
               <ref name="correspondence_model.addressLike"/>
