               <ref name="correspondence_model.global"/>
            </zeroOrMore>
            <optional>
               <ref name="correspondence_back"/>
               <zeroOrMore>
                  <ref name="correspondence_model.global"/>
               </zeroOrMore>
            </optional>
         </group>
         <ref name="correspondence_att.global.attributes"/>
         <ref name="correspondence_att.declaring.attributes"/>
         <ref name="correspondence_att.typed.attributes"/>
