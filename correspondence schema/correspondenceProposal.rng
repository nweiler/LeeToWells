            <choice>
               <group>
                  <zeroOrMore>
                     <ref name="correspondence_model.pLike"/>
                  </zeroOrMore>
               </group>
               <zeroOrMore>
                  <choice>
                     <ref name="correspondence_model.labelLike"/>
                     <ref name="correspondence_model.nameLike"/>
                     <ref name="correspondence_model.placeLike"/>
                  </choice>
               </zeroOrMore>
            </choice>
            <zeroOrMore>
               <choice>
                  <ref name="correspondence_model.noteLike"/>
                  <ref name="correspondence_model.biblLike"/>
               </choice>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="correspondence_model.personLike"/>
            </zeroOrMore>
         </group>
         <ref name="correspondence_att.global.attributes"/>
         <ref name="correspondence_att.typed.attributes"/>
         <ref name="correspondence_att.editLike.attributes"/>
         <optional>
            <attribute name="role">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies a primary role or classification for the organization.</a:documentation>
