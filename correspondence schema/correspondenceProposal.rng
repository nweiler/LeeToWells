   <define name="correspondence_event">
      <element name="event">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(event) contains data relating to any kind of significant event associated with a person, place, or organization.</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="correspondence_model.headLike"/>
            </zeroOrMore>
            <choice>
               <group>
                  <oneOrMore>
                     <ref name="correspondence_model.pLike"/>
                  </oneOrMore>
               </group>
               <group>
                  <oneOrMore>
                     <ref name="correspondence_model.labelLike"/>
                  </oneOrMore>
               </group>
            </choice>
            <zeroOrMore>
               <choice>
                  <ref name="correspondence_model.noteLike"/>
                  <ref name="correspondence_model.biblLike"/>
               </choice>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="correspondence_event"/>
            </zeroOrMore>
         </group>
         <ref name="correspondence_att.global.attributes"/>
         <ref name="correspondence_att.datable.attributes"/>
         <ref name="correspondence_att.editLike.attributes"/>
         <ref name="correspondence_att.typed.attributes"/>
         <ref name="correspondence_att.naming.attributes"/>
