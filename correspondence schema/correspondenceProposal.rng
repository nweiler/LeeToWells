   <define name="correspondence_langKnown">
      <element name="langKnown">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(language known) summarizes the state of a person's linguistic competence, i.e., knowledge of a single language.</a:documentation>
         <ref name="correspondence_macro.phraseSeq.limited"/>
         <ref name="correspondence_att.global.attributes"/>
         <ref name="correspondence_att.datable.attributes"/>
         <ref name="correspondence_att.editLike.attributes"/>
