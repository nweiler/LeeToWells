            <zeroOrMore>
               <ref name="correspondence_model.headLike"/>
            </zeroOrMore>
            <choice>
               <group>
                  <zeroOrMore>
                     <ref name="correspondence_model.pLike"/>
                  </zeroOrMore>
               </group>
