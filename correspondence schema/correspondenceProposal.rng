               <ref name="correspondence_model.headLike"/>
            </zeroOrMore>
            <choice>
               <group>
                  <oneOrMore>
                     <ref name="correspondence_model.pLike"/>
                  </oneOrMore>
               </group>
               <group>
                  <oneOrMore>
                     <ref name="correspondence_model.labelLike"/>
                  </oneOrMore>
