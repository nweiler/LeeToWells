         <group>
            <zeroOrMore>
               <ref name="correspondence_model.headLike"/>
            </zeroOrMore>
            <oneOrMore>
               <choice>
                  <ref name="correspondence_org"/>
                  <ref name="correspondence_listOrg"/>
               </choice>
            </oneOrMore>
            <zeroOrMore>
               <choice>
                  <ref name="correspondence_relationGrp"/>
                  <ref name="correspondence_relation"/>
               </choice>
            </zeroOrMore>
         </group>
         <ref name="correspondence_att.global.attributes"/>
         <ref name="correspondence_att.typed.attributes"/>
         <ref name="correspondence_att.declarable.attributes"/>
